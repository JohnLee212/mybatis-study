<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwx.customizemybatis.service.UserMapper">

    <select id="selectByPrimaryKey" resultType="com.lwx.customizemybatis.domain.User">
	        select *
	        from t_user
	        where userId = #{userId}
	    </select>

    <select id="selectAll" resultType="com.lwx.customizemybatis.domain.User">
	        select *
	        from t_user
	    </select>

</mapper>